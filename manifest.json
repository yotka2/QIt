    {
    "manifest_version": 3,
    "name": "QIO",
    "version": "0.1",
    "description": "A simple chrome extension that generates QR codes from selected texts",
    "permissions": [
        "contextMenus",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "icons": {
        "128": "images/icon128.png"
    }
}